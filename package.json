{
  "name": "inventory-sync-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn src/infrastructure/http/server.ts",
    "build": "tsc -p tsconfig.json && tsc-alias",
    "start": "node dist/infrastructure/http/server.js",
    "prisma:dev": "prisma migrate dev",
    "seed": "npm run build && node dist/infrastructure/inventory/scripts/seed.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules --require tsconfig-paths/register' jest --runInBand --detectOpenHandles"
  },
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
 "devDependencies": {
  "@types/express": "^4.17.21",
  "@types/swagger-ui-express": "^4.1.8",
  "@types/yamljs": "^0.2.34",
  "@types/jest": "^29.5.14",
  "@types/supertest": "^2.0.16",
  "jest": "^29.7.0",
  "supertest": "^6.3.4",
  "ts-jest": "^29.1.1",
  "prisma": "^5.16.2",
  "ts-node-dev": "^2.0.0",
  "tsc-alias": "^1.8.16",
  "typescript": "^5.4.5"
}
}
